#include <stdio.h>                                                              
#include <stdlib.h>                                                             
                                                                                
int main(void){                                                                 
    double s1, s2, s3;                                                          
    int return_code;                                                            
                                                                                
    printf("Enter scores given by 3 judges: ");                                 
    fflush(stdout);                                                             
    return_code = scanf("%lg, %lg, %lg", s1, s2, s3);                           
    if(return_code != 3) {                                                      
        printf("scanf returned ccode %d\n", return_code);                       
        return EXIT_FAILURE;                                                    
    }                                                                           
                                                                                
    if(s1<s2 && s1<s3) {                                                        
        printf("Contestant's final score: %lg\n", (s2+s3)/2);                   
    }                                                                           
    else if(s2<s1 && s2<s3) {                                                   
        printf("Contestant's final score: %lg\n", (s1+s3)/2);                   
